<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <% if @post.has_photo? %>
    <%= image_tag @post.photo_path %>
  <% else %>
    No se ha ingresado una foto
  <% end %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
